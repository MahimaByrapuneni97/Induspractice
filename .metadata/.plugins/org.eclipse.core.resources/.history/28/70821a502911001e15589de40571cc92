package com.indus.pratice.basics;

import junit.framework.TestCase;

public class TestDivision extends TestCase {
	Calc calObj = null;
	protected void setUp() throws Exception {
		Calc calObj = new Calc();
		super.setUp();
	}

	protected void tearDown() throws Exception {
		calObj = null;
		super.tearDown();
	}
	public void TestDivisionSer1() {
		// 1.input
		double par1 = 20.0;
		double par2 = 10.0;
		// 2.Expected Output
		double expectedOutput = 2.0;
		// 3.Actual Output
		double actualOutput = par1 / par2;
		// 4.Asserts
		assertEquals(expectedOutput, actualOutput, 0);
	}

	public void TestDivisionSer2() {
		// 1.input
		double par1 = 200.0;
		double par2 = 10.0;
		// 2.Expected Output
		double expectedOutput = 20.0;
		// 3.Actual Output
		double actualOutput = par1 / par2;
		// 4.Asserts
		assertEquals(expectedOutput, actualOutput, 0);
	}


}
